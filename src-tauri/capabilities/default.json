    "core:default",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "fs:allow-create",
    "fs:allow-mkdir",
    "fs:allow-exists",
    "fs:allow-rename",
